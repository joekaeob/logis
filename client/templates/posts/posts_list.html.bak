<template name="postsList">
  {{sys_time}}
  <div class="posts page">
	<div class="row">
		<div class="col-md-6">
			<h4 class="text-center"><span class="label label-success label-as-badge">RFID NUMBER : {{rfids_count}}</span></h4>
			<div class="hero-unit" style="height:500px; overflow:hidden; overflow-y:auto;">
				{{#each rfids}}
				  {{> postsItem}}
				{{/each}}
			</div>
		</div>

		<div class="col-md-6">
			<h4 class="text-center"><span class="label label-danger label-as-badge">RFID NUMBER : {{rfids_dup_count}}</span></h4>
		    <div class="hero-unit" style="height:500px; overflow:hidden; overflow-y:auto;">
				{{#each rfids_dup}}
				  {{rfids_dup_display timestamp rfid}}
				  {{> postsItemDup}}
				{{/each}}
			</div>
		</div>
	</div>
  </div>
</template>